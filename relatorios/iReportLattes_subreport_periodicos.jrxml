	<queryString>
		<![CDATA[SELECT
    count(p.title),q.classification, (SELECT
    count(p.title)
FROM
    lattes.publication p
        INNER JOIN
    lattes.publicationtype pt ON pt.identifier = p.publicationType_identifier
WHERE
    pt.type = 0)as 'total'
FROM
    lattes.publication p
        INNER JOIN
    lattes.publicationtype pt ON pt.identifier = p.publicationType_identifier
        INNER JOIN
    lattes.qualis q ON q.name = pt.name
WHERE
    pt.type = 0
GROUP BY q.classification;]]>
